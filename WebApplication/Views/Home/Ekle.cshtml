@model Entities.Tesis

@{
    ViewData["Title"] = "Ekle";

}
<div class="jumbotron-fluid">
    <h1>Ekle</h1>

    <h4>Tesis</h4>
    <hr/>
    <div class="row">
        <div class="col-md-4">
            <form asp-action="Ekle" enctype="multipart/form-data">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="Name" class="control-label"></label>
                    <input asp-for="Name" class="form-control"/>
                    <span asp-validation-for="Name" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Adres" class="control-label"></label>
                    <input asp-for="Adres" class="form-control"/>
                    <span asp-validation-for="Adres" class="text-danger"></span>
                </div>

                <div class="form-group">
                    <input type="submit" value="Ekle" class="btn btn-primary"/>
                </div>
            </form>
        </div>
    </div>

    <div>
        <a asp-action="Index">Listeye geri dön</a>
    </div>
</div>